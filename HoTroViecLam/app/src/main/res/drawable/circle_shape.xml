<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- Màu nền của hình tròn -->
    <solid android:color="#A6F0A9"/> <!-- Màu cam đậm -->

    <!-- Đường viền của hình tròn (nếu cần) -->
    <stroke
        android:width="2dp"
        android:color="#FFFFFF"/> <!-- Màu trắng -->

    <!-- Padding (nếu cần) -->
    <padding
        android:left="4dp"
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp" />

    <!-- Kích thước mặc định (nếu cần) -->
    <size
        android:width="100dp"
        android:height="100dp" />

</shape>
